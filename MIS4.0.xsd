			<xs:enumeration value="prj">
				<xs:annotation>
					<xs:documentation>Projecte</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="que">
				<xs:annotation>
					<xs:documentation>Queixa</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
				<xs:enumeration value="reb">
				<xs:annotation>
					<xs:documentation>Rebut</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="rec">
				<xs:annotation>
					<xs:documentation>Reclamació</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="reu">
				<xs:annotation>
					<xs:documentation>Recurs</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="reg">
				<xs:annotation>
					<xs:documentation>Registre</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="rel">
				<xs:annotation>
					<xs:documentation>Reglament</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="req">
				<xs:annotation>
					<xs:documentation>Requeriment</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="res">
				<xs:annotation>
					<xs:documentation>Resolució</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ret">
				<xs:annotation>
					<xs:documentation>Retall De Premsa</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="sal">
				<xs:annotation>
					<xs:documentation>Saluda</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="sol">
				<xs:annotation>
					<xs:documentation>Sol·licitud</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="tel">
				<xs:annotation>
					<xs:documentation>Telegrama</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!--+++++++++-->
	<!-- MIS     -->
	<!--+++++++++-->
	<xs:element name="Formulari">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="CapFormulari">
					<xs:complexType>
						<xs:sequence>
							<!--veure esquema CodiLlistaIdiomasISO639-1.xsd -->
							<xs:element name="Idioma" type="TypeCodiIdiomaISO639" default="ca"/>
							<xs:element name="Origen" type="TypeOrigen"/>
							<xs:element name="Desti" type="TypeDesti"/>
							<xs:element name="ClassificacioFuncional" type="TypeFuncional"/>
							<xs:element name="Seguretat" type="TypeSeguretat" minOccurs="0"/>
							<xs:element name="InfoSignaturaDocument" type="TypeInfoSignaturaDocument" minOccurs="0" maxOccurs="unbounded"/>
							<xs:element name="Observacions" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0"/>
										<xs:maxLength value="500"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="Hash" minOccurs="0">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:string">
											<xs:attribute name="Algoritme" type="xs:string" use="required"/>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="VersioForm" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="5"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="CodiInstanciaTramit">
								<xs:simpleType>
									<xs:restriction base="xs:string"/>
								</xs:simpleType>
							</xs:element>
							<xs:element name="CodiInstanciaTramitPredecessor">
								<xs:simpleType>
									<xs:restriction base="xs:string"/>
								</xs:simpleType>
							</xs:element>
							<xs:element name="FilExecucio">
								<xs:simpleType>
									<xs:restriction base="xs:string"/>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="CosFormulari">
					<xs:complexType>
						<xs:sequence>
							<!--+++++++++++++++++++++++++++++++++++-->
							<!--Elements específics de cada un dels formularis-->
							<!--+++++++++++++++++++++++++++++++++++-->
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--++++++++++++-->
	<!-- Tipus Origen -->
	<!--++++++++++++-->
	<xs:complexType name="TypeOrigen">
		<xs:sequence>
			<xs:element name="IdentificadorOrigen">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="TypeIdentificador">
							<xs:attribute name="TipusDocumentOrigen" type="TypeTipusDocument" use="required"/>
							<xs:attribute name="PaisDocumentOrigen" type="TypeCodiLlistaPaisosISO3166" use="optional"/>
						</xs:extension>
						<!--Type al esquema CodiLlistaPaisosISO3166-1.xsd-->
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<!-- l'aparició d'aquest element està condicionat a que l'ens sigui administració pública-->
			<!-- l'aparició d'aquest element està condicionat a que l'ens sigui administració pública-->
			<xs:element name="SubIdentificador" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="TypeSubIdentificador">
							<xs:attribute name="TipusSubIdentificador" type="TypeTipusSubIdentificador" use="required"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="CodiINE10" type="TypeCodiINE10" minOccurs="0"/>
			<xs:element name="DadesRegistre" type="TypeDadesRegistre" minOccurs="0"/>
			<xs:element name="ResultatRegistre" type="TypeResultatRegistre" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="TipusOrigen" use="optional">
			<!-- atribut 'optional' per compatibilitat amb OVER1 -->
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="persona_fisica"/>
					<xs:enumeration value="entitat"/>
					<xs:enumeration value="institucio"/>
					<!-- El tipus de subjecte correspon a una persona física-->
					<!-- El tipus de subjecte correspon a una organització d'àmbit privat-->
					<!-- El tipus de subjecte correspon a una organització d'àmbit públic-->
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<!--++++++++++-->
	<!-- Tipus Desti   -->
	<!--++++++++++-->
	<xs:complexType name="TypeDesti">
		<xs:sequence>
			<xs:element name="IdentificadorDesti" type="TypeIdentificador"/>
			<xs:element name="SubIdentificador" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="TypeSubIdentificador">
							<xs:attribute name="TipusSubIdentificador" type="TypeTipusSubIdentificador" use="required"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="CodiINE10" type="TypeCodiINE10" minOccurs="0"/>
			<xs:element name="DadesRegistre" type="TypeDadesRegistre" minOccurs="0"/>
			<xs:element name="ResultatRegistre" type="TypeResultatRegistre" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="TipusDesti" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="entitat"/>
					<xs:enumeration value="institucio"/>
					<!-- El tipus de subjecte correspon a una organització d'àmbit privat-->
					<!-- El tipus de subjecte correspon a una organització d'àmbit públic-->
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<!--+++++++++++-->
	<!-- Tipus ResultatRegistre-->
	<!--++++++++++++-->
	<xs:complexType name="TypeResultatRegistre">
		<xs:sequence>
			<xs:element name="DataRegistre">
				<xs:simpleType>
					<xs:restriction base="xs:dateTime"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AssentamentRegistre">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="EvidenciaRegistre" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:base64Binary"/>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="TipusRegistre" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="20"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<!--++++++++++-->
	<!--Tipus Funcional-->
	<!--++++++++++-->
	<xs:complexType name="TypeFuncional">
		<xs:sequence>
			<xs:element name="CodiCataleg" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CodiFuncionalServei" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CodiFuncional">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NomProcediment">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="150"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!-- Type al esquema CodiLlistaTipologiesDocumentals .xsd-->
			<xs:element name="TipusDocument" type="TypeLlistaTipologiesDocumentals"/>
		</xs:sequence>
	</xs:complexType>
	<!--++++++++++++-->
	<!--Tipus Seguretat-->
	<!--++++++++++++-->
	<xs:complexType name="TypeSeguretat">
		<xs:sequence>
			<xs:element name="SensibilitatDadesLOPD">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="basic"/>
						<xs:enumeration value="mig"/>
						<xs:enumeration value="alt"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NivellClassificacioEvidencial">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:pattern value="[0-6]"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NivellSeguretat">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="public"/>
						<xs:enumeration value="intern"/>
						<xs:enumeration value="restringit"/>
						<xs:enumeration value="confidencial"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--++++++++++++++++++++++-->
	<!--Tipus InfoSignaturaDocument -->
	<!--++++++++++++++++++++++-->
	<xs:complexType name="TypeInfoSignaturaDocument">
		<xs:sequence>
			<xs:element name="TipusSignatura">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="embolcallant"/>
						<xs:enumeration value="incrustada"/>
						<xs:enumeration value="independent"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="SignaturaValida">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="si"/>
						<xs:enumeration value="no"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DataValidacio" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:dateTime"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="EvidenciaValidacio" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DadesSignatari">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="NomSignatari" type="TypeNomSignatari"/>
						<xs:element name="CognomsSignatari" type="TypeCognomsSignatari"/>
						<xs:element name="IdentificacioSignatari" type="TypeIdentificadorSignatari"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--+++++++++++++++-->
	<!--Tipus Indentificador  -->
	<!--+++++++++++++++-->
	<xs:simpleType name="TypeIdentificador">
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<!--++++++++++++-->
	<!--Tipus Document -->
	<!--++++++++++++-->
	<xs:simpleType name="TypeTipusDocument">
		<xs:restriction base="xs:string">
			<xs:enumeration value="nif"/>
			<xs:enumeration value="nie"/>
			<xs:enumeration value="cif"/>
			<xs:enumeration value="altres"/>
		</xs:restriction>
	</xs:simpleType>
	<!--++++++++++++++-->
	<!-- Tipus Sub identitat-->
	<!--++++++++++++++-->
	<xs:simpleType name="TypeSubIdentificador">
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<!--+++++++++++++++++++++++++++++++++++++++++++-->
	<!-- Tipus Tipus Sub Identificador tant per origen com per desti-->
	<!--+++++++++++++++++++++++++++++++++++++++++++-->
	<xs:simpleType name="TypeTipusSubIdentificador">
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<!--++++++++++++++-->
	<!-- Tipus Codi INE 10 -->
	<!--++++++++++++++-->
	<xs:simpleType name="TypeCodiINE10">
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<!--+++++++++++++++++-->
	<!-- Tipus unitat de registre  -->
	<!--+++++++++++++++++-->
	<xs:simpleType name="TypeUnitatRegistre">
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<!--+++++++++++++++++++++++++++++++++++-->
	<!-- Tipus oficina registre  per a registres telemàtics -->
	<!--+++++++++++++++++++++++++++++++++++-->
	<xs:simpleType name="TypeOficinaRegistre">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{4}"/>
		</xs:restriction>
	</xs:simpleType>
	<!--+++++++++++++++-->
	<!-- Tipus Nom Signatari-->
	<!--+++++++++++++++-->
	<xs:simpleType name="TypeNomSignatari">
		<xs:restriction base="xs:string">
			<xs:maxLength value="35"/>
		</xs:restriction>
	</xs:simpleType>
	<!--++++++++++++++++++-->
	<!-- Tipus Cognoms Signatari-->
	<!--+++++++++++++++++++-->
	<xs:simpleType name="TypeCognomsSignatari">
		<xs:restriction base="xs:string">
			<xs:maxLength value="100"/>
		</xs:restriction>
	</xs:simpleType>
	<!--+++++++++++++++++++++-->
	<!-- Tipus Identificador Signatari-->
	<!--+++++++++++++++++++++-->
	<xs:simpleType name="TypeIdentificadorSignatari">
		<xs:restriction base="xs:string">
			<xs:maxLength value="15"/>
		</xs:restriction>
	</xs:simpleType>
	<!--+++++++++++++++++++++-->
	<!-- Tipus DadesRegistre-->
	<!--+++++++++++++++++++++-->
	<xs:complexType name="TypeDadesRegistre">
		<xs:sequence>
			<xs:element name="UnitatRegistre" type="TypeUnitatRegistre"/>
			<xs:element name="OficinaRegistre" type="TypeOficinaRegistre" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
